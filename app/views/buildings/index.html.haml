%h1 Buildings#index
%p Find me in app/views/buildings/index.html.haml

= @buildings.count

= search_form_for @q do |f|
  = f.search_field :name_or_nick_name_or_abbreviation_cont
  -# = f.search_field :nick_name_or_abbreviation_cont

- @buildings.each do |b|
  %li= b.name

= content_tag 'comp-wrapper', nil, data: { component: 'buildings_search', props: { buildings: @buildings } }.to_json
