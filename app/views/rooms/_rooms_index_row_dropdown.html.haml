.classroom-row-dropdown.hidden{data: {target: "dropdown.menu"}}
  %div
    -# .relative
      -# .absolute.inset-0.bg-gray-100{class: "h-1/2"}
    .relative.mx-auto{class: ""}
      .mx-auto.rounded-lg.shadow-lg.overflow-hidden.pb-10{class: "md:max-w-none md:flex"}
        .classroom-row-dropdown-features-section

          .classroom-row-dropdown-features-group
            %h4.flex-shrink-0.pr-4.text-sm.leading-5.tracking-wider.font-semibold.uppercase.text-blue-600
              Features
